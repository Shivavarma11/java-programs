class Solution {
    public boolean rotateString(String s, String goal) {
        int n1=s.length();
        int n2=goal.length();
        if(n1>n2)
        {
            return false;
        }
        String str=goal+goal;
         
        if(str.contains(s))
        {
            return true;
        }
        else
        {
            return false;
        }
        
    }
}
