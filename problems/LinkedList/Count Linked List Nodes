/*
class Node{
    int data;
    Node next;
    Node(int a){  data = a; next = null; }
}*/

class Solution {
    // Function to count nodes of a linked list.
    public int getCount(Node head) {
        // code here
        int len=0;
        Node curr=head;
        while(curr!=null)
        {
            len++;
            curr=curr.next;
        }
        return len;
    }
}
